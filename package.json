    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "better-sqlite3": "^11.10.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.43.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7"
  }
